#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/kali/Age_of_Bitches3/node_modules/.pnpm/pseudolocale@2.1.0/node_modules/pseudolocale/dist/node_modules:/home/kali/Age_of_Bitches3/node_modules/.pnpm/pseudolocale@2.1.0/node_modules/pseudolocale/node_modules:/home/kali/Age_of_Bitches3/node_modules/.pnpm/pseudolocale@2.1.0/node_modules:/home/kali/Age_of_Bitches3/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/home/kali/Age_of_Bitches3/node_modules/.pnpm/pseudolocale@2.1.0/node_modules/pseudolocale/dist/node_modules:/home/kali/Age_of_Bitches3/node_modules/.pnpm/pseudolocale@2.1.0/node_modules/pseudolocale/node_modules:/home/kali/Age_of_Bitches3/node_modules/.pnpm/pseudolocale@2.1.0/node_modules:/home/kali/Age_of_Bitches3/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../pseudolocale/dist/cli.mjs" "$@"
else
  exec node  "$basedir/../pseudolocale/dist/cli.mjs" "$@"
fi
